        "@types/validator": "13.0.0",
        "@types/webpack-env": "1.15.1",
        "@uifabric/icons": "7.3.27",
        "@uifabric/react-hooks": "^7.2.0",
        "apollo-cache": "1.3.4",
        "apollo-cache-inmemory": "1.6.5",
        "apollo-client": "2.6.8",
        "apollo-link": "1.2.13",
        "apollo-link-http": "1.5.17",
