{
    "name": "did365",
    "version": "0.3.1",
    "private": true,
    "engines": {
        "node": ">=10.14.1",
        "npm": ">=6.4.1"
    },
    "scripts": {
        "watch": "concurrently \"npm run watch:client:package\" \"npm run watch:server\" \"npm run watch:scss\"",
        "start": "node ./bin/www",
        "watch:server": "nodemon --ignore client --ignore public --ignore lib ./bin/www",
        "build:scss": "node-sass --include-path scss scss/main.scss public/css/main.css",
        "watch:scss": "onchange -k -i -d 750 \"./**/*.scss\" -- npm run build:scss",
        "package:client": "tsc --build && webpack --config webpack.config.js",
        "package:client:prod": "tsc --build && webpack --config webpack.config.prod.js",
        "watch:client:package": "onchange -k -i -d 750 \"./client/**/*.ts\" \"./client/**/*.tsx\" -- npm run package:client",
        "clean": "rimraf ./lib && rimraf ./public/js"
    },
    "dependencies": {
        "@microsoft/applicationinsights-web": "2.5.0",
        "@microsoft/microsoft-graph-client": "2.0.0",
        "@pnp/common": "1.3.7",
        "@types/react-autosuggest": "^9.3.13",
        "apollo-cache": "1.3.4",
        "apollo-cache-inmemory": "1.6.5",
        "apollo-client": "2.6.8",
        "apollo-link": "^1.2.13",
        "apollo-link-http": "1.5.16",
        "apollo-utilities": "1.3.3",
        "array-sort": "^1.0.0",
        "autosuggest-highlight": "3.1.1",
        "azure-storage": "2.10.3",
        "chalk": "3.0.0",
        "connect-azuretables": "1.0.20",
        "connect-flash": "0.1.1",
        "cookie-parser": "~1.4.4",
        "debug": "^4.1.1",
        "dotenv": "8.2.0",
        "express": "4.17.1",
        "express-favicon": "^2.0.1",
        "express-graphql": "0.9.0",
        "express-pino-logger": "^4.0.0",
        "express-session": "1.17.0",
        "get-value": "3.0.1",
        "graphql": "14.6.0",
        "graphql-import": "0.7.1",
        "graphql-tag": "2.10.3",
        "graphql-tools": "4.0.7",
        "hbs": "4.1.0",
        "helmet": "^3.21.2",
        "highcharts": "8.0.2",
        "highcharts-react-official": "3.0.0",
        "http-errors": "1.7.3",
        "loglevel": "1.6.7",
        "loglevel-plugin-prefix": "0.8.4",
        "moment": "2.24.0",
        "morgan": "1.9.1",
        "node-fetch": "2.6.0",
        "office-ui-fabric-react": "7.98.4",
        "passport": "0.4.1",
        "passport-azure-ad": "4.2.1",
        "pino": "^5.14.0",
        "react": "16.13.0",
        "react-apollo": "3.1.3",
        "react-autosuggest": "^9.4.3",
        "react-dom": "16.13.0",
        "react-markdown": "4.3.1",
        "scriptjs": "2.5.9",
        "simple-oauth2": "2.4.0",
        "string-format": "^2.0.0",
        "string-similarity": "^4.0.1",
        "string-strip-html": "4.3.16",
        "supports-color": "^7.1.0",
        "twix": "^1.3.0",
        "underscore": "^1.9.1",
