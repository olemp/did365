{
    "name": "did365",
    "version": "0.3.1",
    "private": true,
    "engines": {
        "node": ">=10.14.1",
        "npm": ">=6.5.0"
    },
    "config": {
        "client": "./lib/client"
    },
    "scripts": {
        "watch": "concurrently \"npm run watchPackageClient\" \"npm run watchServer\" \"npm run watchStyles\"",
        "start": "node ./bin/www",
        "watchServer": "nodemon --ignore client --ignore public --ignore lib ./bin/www",
        "packageStyles": "node-sass --include-path scss scss/main.scss public/css/main.css",
        "watchStyles": "onchange -k -i -d 250 \"./**/*.scss\" -- npm run packageStyles",
        "packageClient": "tsc --build && webpack --config webpack.config.js",
        "watchPackageClient": "onchange -k -i -d 250 \"./client/**/*.ts\" \"./client/**/*.tsx\" -- npm run packageClient",
        "makeDoc": "typedoc --options typedoc.json"
    },
    "dependencies": {
        "@apollo/react-hooks": "3.1.3",
        "@babel/compat-data": "7.8.0",
        "@babel/core": "7.8.6",
        "@babel/preset-env": "7.8.0",
        "@microsoft/applicationinsights-web": "2.5.4",
        "@microsoft/microsoft-graph-client": "2.0.0",
        "@pnp/common": "2.0.4",
        "@types/autosuggest-highlight": "3.1.0",
        "@types/get-value": "3.0.1",
