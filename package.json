        "@types/webpack-env": "1.15.1",
        "@uifabric/icons": "7.3.14",
        "@uifabric/utilities": "^7.6.0",
        "babel-loader": "8.0.6",
        "concurrently": "5.1.0",
        "core-js": "3.6.4",
